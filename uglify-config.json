{
    "alwaysRun": true,
    "recursiveFolderSearch": true,
    "foldersToProcess": [
        "scripts"
    ],
    "filesToExclude": [
        "scripts/index.js"
    ],
    "uglifyJsOptions": {
        "compress": {
            "drop_console": true
        },
        "mangle": false,
        "output": {
            "code": true
        }
    },
    "cleanCssOptions": {
        "specialComments": "all"
    }
}
